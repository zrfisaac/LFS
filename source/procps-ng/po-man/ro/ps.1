%G	group	GROUP
%P	ppid	PPID
%U	utilizator	USER
%a	args	COMANDA
%c	comm	COMANDA
%g	rgroup	RGROUP
%n	nice	NI
%p	pid	PID
%r	pgid	PGID
%t	etime	ELAPSED
