msgid ""
"B<kill>(1), B<killall>(1), B<stat>(2), B<btrfs>(5), B<lsof>(8), "
"B<mount_namespaces>(7), B<pkill>(1), B<ps>(1), B<kill>(2)."
msgstr ""
"B<kill>(1), B<killall>(1), B<lsof>(8), B<mount_namespaces>(7), B<pkill>(1), "
