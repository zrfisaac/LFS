#. type: Plain text
#: ../../man/man1/apropos.man1:32
msgid ""
"B<%apropos%> [\\|B<-dalv?V>\\|] [\\|B<-e>\\||\\|B<-w>\\||\\|B<-r>\\|] [\\|B<-"
"s> I<list>\\|] [\\|B<-m> I<system>\\|[\\|,.\\|.\\|.\\|]\\|] [\\|B<-M> "
"I<path>\\|] [\\|B<-L> I<locale>\\|] [\\|B<-C> I<file>\\|] I<keyword> \\&.\\|."
"\\|."
msgstr ""
"B<%apropos%> [\\|B<-dalv?V>\\|] [\\|B<-e>\\||\\|B<-w>\\||\\|B<-r>\\|] [\\|B<-"
"s> I<Liste>\\|] [\\|B<-m> I<System>\\|[\\|,.\\|.\\|.\\|]\\|] [\\|B<-M> "
"I<Pfad>\\|] [\\|B<-L> I<Locale>\\|] [\\|B<-C> I<Datei>\\|] I<Schlüsselwort> "
"\\&.\\|.\\|."

#. type: SH
